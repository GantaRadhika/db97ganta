extends layout

block content
  h1= title
  - let Eggfry = results;
  script.
    function buy(x){
      alert('Purchased Item: ' + x)
   } 
  div(class='container-fluid')
    div(class='row')
      each m, index in Eggfry 
       div(class="col-sm-2" onclick=`buy(${index})`) #{m.Onions} #{m.spices} #{m.curry} 